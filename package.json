    "regenerator-runtime": "0.11.1",
    "source-map-support": "0.5.3",
    "spectron": "3.8.0",
    "standard-version": "4.3.0",
    "style-loader": "0.20.3",
    "stylelint": "8.4.0",
    "svg-sprite-loader": "3.7.3",
    "trash-cli": "1.4.0",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
