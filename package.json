    "jest": "22.4.3",
    "jsdom": "11.6.2",
    "json-loader": "0.5.7",
    "less": "2.7.3",
    "less-loader": "4.1.0",
    "minimist": "1.2.0",
    "mobx": "4.1.1",
    "mobx-react": "5.0.0",
    "mobx-react-devtools": "5.0.1",
    "prop-types": "15.6.1",
